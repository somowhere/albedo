	<!-- java类型 -->
	<javaType>
		<dict val="String" name="String"/>
		<dict val="Long" name="Long"/>
		<dict val="Integer" name="Integer"/>
		<dict val="Double" name="Double"/>
		<dict val="java.util.Date" name="Date"/>
