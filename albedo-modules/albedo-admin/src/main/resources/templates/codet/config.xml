		<dict val="likeLeft" name="likeLeft"/>
		<dict val="likeRight" name="likeRight"/>
		<dict val="notLike" name="notLike"/>
		<dict val="in" name="in"/>
		<dict val="notIn" name="notIn"/>
		<dict val="between" name="between"/>
	</queryType>
